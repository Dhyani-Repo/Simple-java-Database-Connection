<!DOCTYPE web-app PUBLIC '-//Sun Microsystems, Inc.//DTD Web 
     Application 2.3//EN' 'http://java.sun.com/dtd/web-app_2_3.dtd'>

<web-app>
    <servlet>
         <servlet-name>Servlet1</servlet-name>
         <servlet-class>com.dev.testing.testing</servlet-class>
     </servlet>
   
    <servlet>
         <servlet-name>Servlet2</servlet-name>
         <servlet-class>com.dev.testing.testing</servlet-class>
     </servlet>
    <servlet>
         <servlet-name>Servlet3</servlet-name>
         <servlet-class>com.dev.testing.testing2</servlet-class>
     </servlet>
  
  <servlet-mapping>
       <servlet-name>Servlet1</servlet-name>
       <url-pattern>/checking</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
       <servlet-name>Servlet2</servlet-name>
       <url-pattern>/sending</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
       <servlet-name>Servlet3</servlet-name>
       <url-pattern>/getCookie</url-pattern>
  </servlet-mapping>

</web-app>